{
  "name": "english-master-app",
  "version": "2.0.0",
  "description": "English learning app with personalized AI exercises for Spanish speakers",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "deploy": "npm run build && vercel --prod"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "firebase": "^10.7.1",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.303.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "english-learning",
    "ai-education",
    "spanish-speakers",
    "personalized-learning",
    "react",
    "typescript",
    "firebase",
    "gemini-ai"
  ],
  "author": "English Master Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/english-master-app.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/english-master-app/issues"
  },
  "homepage": "https://english-master-app.vercel.app"
}